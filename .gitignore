import environment from '#core/config/environment.js';

export default async function feeScheduleGetEndpoint(req, res) {
  try {
    const { type } = req.params;

    // Acessando as URLs diretamente do environment
    const scheduleUrls = environment[this.#env]?.scheduleUrls;
    if (!scheduleUrls[type]) {
      return res.status(400).json({ error: 'Invalid schedule type' });
    }

    const url = scheduleUrls[type];
    res.status(200).json({ url });
  } catch (error) {
    res.status(500).json({ error: 'Internal Server Error' });
  }
}
