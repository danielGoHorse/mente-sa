import { mssql } from "./mssql"

mongoose.Promise = global.Promise

export const startConnectionWithSql = ({ server, user, password, database }) => {
  const config = {
    server,
    user,
    password,
    database,
    options: {
      encrypt: false,
      trustServerCertificate: true,
    },
  };
  try {
    mssql.close();
    const connection = mssql.connect(config);
    console.log('Connect to SQL Server successfully');
    return connection;

  } catch (err) {
    console.log('Error connecting to SQL Server', err.message);
    throw err;
  }


}
