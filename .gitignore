function sqlToFeeType({ FeeCode, BPSFeeTypeCode }){
  const byFeeCode = { JH001:1, JH002:2, JH010:3, JH008:4, BPS002:6, BPS003:6, BPS004:6, BPS005:6 };
  const byBpsCode = { S:5, C:6, E:7, O:7 };   // se ainda precisar
  if (FeeCode && byFeeCode[FeeCode])       return byFeeCode[FeeCode];
  if (BPSFeeTypeCode && byBpsCode[BPSFeeTypeCode]) return byBpsCode[BPSFeeTypeCode];
  return 0;
}



const fieldCode = sqlToFeeType(r);   // r Ã© a row da procedure
(acc[fieldCode] ??= []).push(r);
