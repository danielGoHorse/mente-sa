import { executeStoreProcedure } from '#src/core/data/mssql/index.js';

// Função para buscar a equipe de Equity
export async function getEquityTeam(groupId) {
  const query = 'sp_get_equity_team'; // Nome da stored procedure
  const parameters = { group_id: groupId }; // Parâmetro de exemplo

  try {
    const result = await executeStoreProcedure(global.sqlConnection, query, parameters);
    return result; // Retorna o resultado da query
  } catch (err) {
    console.error('Error fetching Equity Team:', err.message);
    throw err;
  }
}
