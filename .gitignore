this.#accounts.forEach((acc) => {
  if (acc.ddaNumber) {
    this.#accountsDDA.push({
      number: acc.ddaNumber,
      code: 5,
      entity: acc.entity || null,
    });
    this.#accountsDDA.push({
      number: acc.ddaNumber,
      code: 7,
      entity: acc.entity || null,
    });
  }
});
console.log('[DEBUG] #accountsDDA before deduplication:', JSON.stringify(this.#accountsDDA, null, 2));

this.#accountsDDA = Array.from(
  new Map(this.#accountsDDA.map(item => [`${item.number}-${item.code}`, item])).values()
);
console.log('[DEBUG] #accountsDDA after deduplication:', JSON.stringify(this.#accountsDDA, null, 2));
