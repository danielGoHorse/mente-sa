// âžœ src/utils/normalizeStatus.ts
export function normalizeStatus(s: string | undefined) {
  if (!s) return "-";
  const map: Record<string, string> = {
    done:       "completed",   // ðŸ”¸ novo alias
    success:    "completed",
    cancel:     "rejected",
    rejected:   "rejected",
    return:     "partial",
    waiting:    "waiting",
    pending:    "pending",
    skipped:    "skipped",
    completed:  "completed",
  };
  return map[s] ?? s;
}
