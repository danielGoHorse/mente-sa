async get(path) {
    console.log("ğŸ”¹ Base URL:", this.#baseUrl);
    console.log("ğŸ”¹ Path:", path);

    try {
        const url = new URL(path, this.#baseUrl);
        console.log("ğŸ”¹ URL Final:", url.toString());

        const response = await this.#request.get(url.toString());
        const responseJson = response.data;
        if (response.status !== 200) {
            return [responseJson, null];
        }

        return [null, responseJson];
    } catch (err) {
        console.error("âŒ Erro ao construir a URL:", err.message);
        return ["Erro ao construir a URL", null];
    }
}
