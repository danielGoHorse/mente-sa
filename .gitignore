function groupApprovalSteps(steps: any[]) {
  // Agrupa os steps por tipo (Callback, Credit, etc)
  const grouped: { [key: string]: any[] } = {};

  steps.forEach((step) => {
    // Extrai o nome "Callback", "Credit", etc
    const match = step.name.match(/^Approvals and Restrictions: (.+?)\s?\d*$/);
    const key = match ? match[1].trim() : step.name;
    if (!grouped[key]) grouped[key] = [];
    grouped[key].push(step);
  });

  return grouped;
}
