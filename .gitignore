import { configureSchemaMongoToReturnId } from '#src/core/data/mongoSchemaBase.js'
import mongoose, { Schema } from 'mongoose'

export const EquityTeamSchema = new Schema(
  {
    userName: {
      type: String,
      required: true
    },
    userNameFull: {
      type: String,
      required: true
    },
    email: {
      type: String,
      requireid: true
    }
  },
  {
    collection: 'EquityTeam',
    timestamps: true
  }
)

configureSchemaMongoToReturnId('toObject', EquityTeamSchema)

export const EquityTeam = mongoose.model('EquityTeam', EquityTeamSchema)


