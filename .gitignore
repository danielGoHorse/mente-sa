// src/app/(private)/cancel-screen/page.tsx
export default function CancelScreen() {
  const { data } = useCancelRequestProvider(); // exemplo

  if (!data) {
    return null; // ou um skeleton/loading
  }

  return (
    <label className="font-[Figtree] text-[16px] font-semibold">
      The request <span>#{data.caseNumber}</span> has been cancelled.
    </label>
  );
}



