#mapCurrentFees(feesData) {
    return feesData
        .filter(record => record.FeeAmount > 0 && record.FeeDescription === "FX Outgoing Wire Fee") // 🔍 Filtra apenas FX Outgoing Wire Fee
        .map(record => ({
            feeDescription: `${record.FeeDescription} - $${record.FeeAmount}.00`,
            feeCode: record.FeeType
        }));
}
